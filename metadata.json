{
  "name": "ghoneycutt-redhat",
  "version": "1.5.0",
  "author": "ghoneycutt",
  "summary": "Manage RedHat based systems",
  "description": "Manage RedHat (or where osfamily is redhat) specific bits",
  "license": "Apache-2.0",
  "source": "git@github.com:ghoneycutt/puppet-module-redhat.git",
  "project_page": "https://github.com/ghoneycutt/puppet-module-redhat",
  "issues_url": "https://github.com/ghoneycutt/puppet-module-redhat/issues",
  "requirements": [
    {
      "name": "pe",
      "version_requirement": ">= 3.2.0 < 5.0.0"
    },
    {
      "name": "puppet",
      "version_requirement": ">= 3.0.0 < 5.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "OracleLinux",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Scientific",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    }
  ],
  "dependencies": [
    {"name":"puppetlabs/stdlib","version_requirement":">= 4.6.0 < 6.0.0"}
  ]
}
